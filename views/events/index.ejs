<h1>Your events:</h1>

<% events.forEach(function(event) { %>
  <div class="well">
  <h2><a href="/events/<%= event.id %>"><%= event.type %></a></h2>
  <h2><a href="/events/<%= event.id %>"><%= event.location %></a></h2>
  <h2><a href="/events/<%= event.id %>"><%= event.time %></a></h2>
  <h2><a href="/events/<%= event.id %>"><%= event.with %></a></h2>
  <h2><a href="/events/<%= event.id %>"><%= event.reason %></a></h2>
  </div>
  <% }); %>

  <hr />
  
  
<a href="/events/new">Create a New Event</a>
  <a href="/profile">&larr; Back To Profile</a>